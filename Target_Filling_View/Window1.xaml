<Window x:Class="Target_Filling_View.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Target_Filling_View"
        mc:Ignorable="d"
        Title="Window1" d:DesignHeight="300" d:DesignWidth="800"
        Loaded="Window1_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        
        <DockPanel x:Name="dckpnlImg"  Background="LightSteelBlue" >
            <Image x:Name="XRayBWImg" Source="{Binding Path=TestImg1}" Stretch="Uniform" Height="700" />
        </DockPanel>
        <DockPanel Grid.Column="1" x:Name="dckpnlImg2"  Background="LightSteelBlue" >
            <Image x:Name="XRayOpenImg" Source="{Binding Path=TestImg2}" Stretch="Uniform" Height="700" />
        </DockPanel>
    </Grid>
</Window>
